{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "es6"
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "object-curly-spacing": [2, "never"],
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "max-len": [2, 120, 2],
    "new-cap": 0,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-param-reassign": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-var": 0,
    "one-var": 0,
    "prefer-const": 0,
    "radix": 0,
    "semi": [2, "always"],
    "space-before-function-paren": 0,
    "valid-jsdoc": 2,
    "prefer-destructuring": 0,
    "strict": 0
  },
  "env": {
    "es6": true,
    "node": true
  }
}
